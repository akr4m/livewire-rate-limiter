{
    "name": "akr4m/livewire-rate-limiter",
    "description": "Flexible rate limiting for Laravel Livewire components with configurable strategies and multiple storage backends",
    "keywords": [
        "laravel",
        "livewire",
        "rate-limiting",
        "throttle",
        "security"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Akram H.",
            "email": "akr4m.kh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/cache": "^11.0|^12.0",
        "livewire/livewire": "^3.5|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Akr4m\\LivewireRateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akr4m\\LivewireRateLimiter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akr4m\\LivewireRateLimiter\\LivewireRateLimiterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
